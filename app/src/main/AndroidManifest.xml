<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="it.bancaprivataleasing.richiestapreventivo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ImportoActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ClienteActivity"
            android:label="@string/cliente_activity" />
        <activity android:name=".AnticipoActivity"
            android:label="@string/anticipo_activity"/>
        <activity android:name=".RiscattoActivity"
            android:label="@string/riscatto_activity"/>
        <activity android:name=".DurataActivity"
            android:label="@string/durata_activity"/>
        <activity android:name=".AssicurazioneActivity"
            android:label="@string/assicurazione_activity">
        </activity>
    </application>

</manifest>